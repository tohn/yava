<h1>Listing j_product_labels</h1>

<div class="center"><%= will_paginate @j_product_labels %></div>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Product</th>
      <th>Label</th>
      <th>User</th>
    </tr>
  </thead>

  <tbody>
    <% @j_product_labels.each do |j_product_label| %>
      <tr>
        <td><%= j_product_label.product.name %></td>
        <td><%= j_product_label.label.name %></td>
        <td><%= j_product_label.user.nickname %></td>
<!--        <td><%#= link_to 'Show', j_product_label %></td>
        <td><%#= link_to 'Edit', edit_j_product_label_path(j_product_label) %></td>
		<td><%#= link_to 'Destroy', j_product_label, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<div class="center"><%= will_paginate @j_product_labels %></div>

<hr>

<p><%= link_to 'New J product label', new_j_product_label_path %></p>
